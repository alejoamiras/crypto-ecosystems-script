# Primary GitHub token
GITHUB_TOKEN=ghp_your_primary_token_here

# Additional tokens for rotation (optional)
# Create multiple personal access tokens to rotate and avoid rate limits
GITHUB_TOKEN_1=ghp_your_second_token_here
GITHUB_TOKEN_2=ghp_your_third_token_here
GITHUB_TOKEN_3=ghp_your_fourth_token_here

# You can add up to GITHUB_TOKEN_10

# Logging configuration
LOG_LEVEL=info
LOG_TO_FILE=false
